<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p" xsi:schemaLocation="http://www.springframework.org/schema/beans  
               http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<!-- Setter Injection-->
	<bean id="res1" class="example01.com.cg.entity.Restaurant">
		<property name="name" value="Mc Donalds"></property>
		<property name="id" value="1"></property>
	</bean>

	<!-- Constructor Injection-->
	<bean id="res2" class="example01.com.cg.entity.Restaurant">
		<constructor-arg value="2"></constructor-arg>
		<constructor-arg value="KFC"></constructor-arg>
	</bean>
</beans>